{
	"name": "react-widget-select",
	"version": "1.0.0",
	"description": "react-widget-select",
	"main": "cjs/index.js",
	"module": "esm/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"cjs",
		"esm",
		"lib",
		"style",
		"*.md"
	],
	"scripts": {
		"docs": "packez build ./examples/index.js -d docs -c",
		"start": "tsc -w",
		"cjs": "transform-es src -d cjs -c -t cjs",
		"esm": "transform-es src -d esm -c -t esm",
		"tsc": "tsc",
		"demo": "packez server ./examples/index.js -d dist -w -c",
		"test": "jest",
		"clear": "rimraf lib cjs esm docs dist",
		"build": "run-s clear tsc cjs esm docs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/react-widget/select.git"
	},
	"keywords": [
		"react",
		"react-widget",
		"select"
	],
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"dependencies": {
		"@babel/runtime": "^7.9.2",
		"classnames": "^2.2.6",
		"react-widget-listbox": "^2.0.1",
		"react-widget-popover": "^1.0.5"
	},
	"author": "nobo.zhou@foxmail.com",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/react-widget/select/issues"
	},
	"homepage": "https://github.com/react-widget/select#readme",
	"devDependencies": {
		"@types/enzyme": "^3.10.5",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/react": "^16.9.34",
		"@types/react-dom": "^16.9.8",
		"babel-plugin-search-and-replace": "^1.0.1",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.24",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"jest": "^25.4.0",
		"npm-run-all": "^4.1.5",
		"packez": "^4.1.3",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"rimraf": "^3.0.2",
		"transform-es": "^5.0.4",
		"typescript": "^3.8.3"
	},
	"browserslist": [
		">=0.25%",
